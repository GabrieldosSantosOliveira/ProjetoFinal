    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rimraf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "type:check": "rimraf ./dist && tsc",
    "format": "prettier -w .",
    "test": "jest --runInBand --verbose"
  },
